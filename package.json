    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "gh-pages": "^6.2.0",
    "jsdom": "^24.1.0",
    "playwright": "^1.47.0",
    "prettier": "^3.3.3",
